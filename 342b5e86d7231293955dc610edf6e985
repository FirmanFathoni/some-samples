<script>
	$(document).ready(function(){
		/// icon meteo
		var icoMeteo = $(".hiddenMeteoTMP img").attr("src");   // STATIC_LPCDN_BASE_URL/lapresse/img/meteo/icone_une/o.png  (o ou on)
		var a_im = icoMeteo.split("/");
		var icoMeteo = a_im[a_im.length -1].replace(".png","").trim()
	
		if(icoMeteo.length > 1){
		   icoMeteo = icoMeteo[0];
		  
		}else{
		   icoMeteo=icoMeteo.toUpperCase()
		  
		}

		// mercure
 		$(".hiddenMeteoTMP span strong").remove();
		var mercure = $(".hiddenMeteoTMP span").text();
		
		// lien Meteo 
		var lienMeteo = $(".hiddenMeteoTMP .ville").attr("href");

		//console.log(icoMeteo,mercure,lienMeteo);

		// append dans le bon div
		$(".meteo .icoMeteo").text(icoMeteo);
		$(".meteo .mercure").html(mercure+$(".meteo .mercure").html());
		$(".meteo").attr("href",lienMeteo);

		// détruire le div temporaire
		$(".hiddenMeteoTMP").remove();

	});

</script>

