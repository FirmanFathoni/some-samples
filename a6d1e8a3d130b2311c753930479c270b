                $('.games-mostplayed div.game').hide().slice(0, 5).show();
                function pagemostplayed(e, start) {
                    $(e).parent().parent().find('li').removeClass('active');
                    $(e).parent().addClass('active');
                    $('.games-mostplayed div.game').hide().slice(start, start + 5).show();
