$(document).ready(function(){
	if(jQuery(window).width()>"768")
		{
			$(document).scroll(function(){
				if(jQuery(window).scrollTop() == 0)
				{
					$(".header-img").css({"position":"absolute","top":"165%","left":"5%","width":"70%"});
				}
				else if(jQuery(window).scrollTop() > 50)
				{		
				   $(".header-img").css({"position":"","top":"0px","left":"0px","width":"148px","margin": "1px 0px 0px 20px"});
				}

			});
		}
	if(jQuery(window).scrollTop() == 0)
				{
					$(".header-img").css({"position":"absolute","top":"165%","left":"5%","width":"70%"});
				}
				else if(jQuery(window).scrollTop() > 50)
				{		
				   $(".header-img").css({"position":"","top":"0px","left":"0px","width":"148px","margin": "1px 0px 0px 20px"});
				}
	window.onresize  = function(){
		if(jQuery(window).width()>"768")
		{
			$(document).scroll(function(){
				if(jQuery(window).scrollTop() == 0)
				{
					$(".header-img").css({"position":"absolute","top":"165%","left":"5%","width":"70%"});
				}
				else if(jQuery(window).scrollTop() > 50)
				{		
				   $(".header-img").css({"position":"","top":"0px","left":"0px","width":"148px","margin": "1px 0px 0px 20px"});
				}

			});

		}
		else
		{
			$(".header-img").css({"position":"","top":"0px","left":"0px","width":"148px","margin": "1px 0px 0px 20px"});
			$(document).unbind('scroll');
	
		}	
	};
		
	jQuery('.nav .scroll').click(function(){		
		jQuery('.navbar-collapse').removeClass('in');
		jQuery('.navbar-collapse').height('1px');

		var targetElem = jQuery(this).children('a').attr('href');
		var scrollTop	= jQuery(targetElem).offset().top;
		jQuery('html,body').animate({'scrollTop':scrollTop},900);
	
	});
});
</script>
