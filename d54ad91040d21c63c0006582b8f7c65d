var slidespeed=1;

function slide(){
	var slider = document.getElementById("logoslider");
	slider_width = window.innerWidth || document.documentElement.clientWidth || document.getElementsByTagName("body")[0].clientWidth;
	if (parseInt(slider.style.left)>50 || parseInt(slider.style.left)<slider_width*-1) { slidespeed = slidespeed * -1; }
	slider.style.left=parseInt(slider.style.left)+slidespeed+"px";
	slider.style.left=parseInt(slider.style.left)+slidespeed+"px";
	slider.style.left=parseInt(slider.style.left)+slidespeed+"px";
}

function openoverlay(which,brand) {
	if (!$("#"+which).is(":visible")) {$("#"+which).fadeToggle(1000);}	
	if (which != "service" && which != "contact" && which != "sitemap") {
		$("#"+which+"_brand").find("option:contains('"+brand+"')").each(function() {
			 if( $(this).text() == brand ) {
			  $(this).attr("selected","selected");
			 }
		});
	}
}

function closeoverlay(which) {
	if ($("#"+which).is(":visible")) {$("#"+which).fadeToggle(1000);}			
}

function resizepage() {
	var w = $(window).width();
	if (w < 1000) {  
		$(".desktop").hide();
		$("#content-box").css({"bottom":"0"});
		if (w < 600) {
			$(".tablet").hide();
		} else {
			$(".tablet").show();
		}
	} else {
		$(".desktop").show();
		$(".tablet").show();
		$("#content-box").css({"bottom":"15%"});
	}
}

$(document).ready(function(){

	$(window).on('resize', function(){
		  resizepage();
	});
	
	resizepage();
	
	slidetimer=setInterval("slide()",30)

	$("#contactform").validationEngine('attach', {promptPosition : "bottomLeft", scroll: false, 
		onValidationComplete: function(form, status){
			if (status) {
				var verificationword = document.getElementById("contact_verificationword").value;
				verificationword = verificationword.toUpperCase();
				if (verificationword == "DIME") {
					var dataString = 'subject=contact&firstname='+ encodeURIComponent(document.getElementById("contact_first_name").value) + '&lastname=' + encodeURIComponent(document.getElementById("contact_last_name").value) + '&company=' + encodeURIComponent(document.getElementById("contact_company").value) + '&city=' + encodeURIComponent(document.getElementById("contact_city").value) + ' ' + encodeURIComponent(document.getElementById("contact_state").value) + '&email=' + encodeURIComponent(document.getElementById("contact_email").value) + '&phone=' + encodeURIComponent(document.getElementById("contact_phone").value) + '&recip_verificationword=' + encodeURIComponent(verificationword) + '&brand=' + '&message=' + encodeURIComponent(document.getElementById("contact_message").value);
					$.ajax({
					  type: "POST",
					  url: "/messagesent/",
					  data: dataString,
					  success: function() {
						$('#contactform').html("<div id='message'></div>");
						$('#message').hide().html("<p style='text-align:center;font-size:50pt; font-family:jennasue; color:#002077;'><img width='80' src='/images/check.png'/><br/>Your request has been sent.</p><p style='text-align:center; font-size:14pt; font-family:Arial;'>We will be in touch soon with your appraisal or any additional questions soon. In the meantime, thank you for choosing Telecom Products Corporation.</p>").fadeIn(1500);
					  }
					});
					
					$.ajax({
					  type: "POST",
					  url: "https://www.salesforce.com/servlet/servlet.WebToLead?encoding=UTF-8",
					  data: 'first_name='+ encodeURIComponent(document.getElementById("contact_first_name").value) + '&last_name=' + encodeURIComponent(document.getElementById("contact_last_name").value) + '&company=' + encodeURIComponent(document.getElementById("contact_company").value) + '&city=' + encodeURIComponent(document.getElementById("contact_city").value) + '&state=' + encodeURIComponent(document.getElementById("contact_state").value) + '&email=' + encodeURIComponent(document.getElementById("contact_email").value) + '&phone=' + encodeURIComponent(document.getElementById("contact_phone").value) + '&description=' + encodeURIComponent(document.getElementById("contact_message").value) + '&oid=00D30000000JOnh'
					});
	
				} else {
					alert("Please enter the type of coin displayed.  Hint: this is the four letter word for a 10 cent American coin.");				
				}	
			} 
		}
	});


