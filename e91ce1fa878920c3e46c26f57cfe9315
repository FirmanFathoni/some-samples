<script>
    function showFormLoader(step) {
        $(step ? "#form-submit-step" + step : "#form-submit").prop('disabled', true);
        $(step ? "#form-loader-step" + step : "#form-loader").show();
    }

    function hideFormLoader(step) {
        $(step ? "#form-submit-step" + step : "#form-submit").prop('disabled', false);
        $(step ? "#form-loader-step" + step : "#form-loader").hide();
    }

    $(document).ready(function() {
        exitBack('http://4ur.click/llexit/?country=fr&networkid=65&publisher=15565_0&service=want-to-win&service_tbl=FR_bbtn&pageid=supermarket2&type=web');

        
        var question = new Question(
            "FR",
            "fr_FR",
            "dfcda66a5470835cc2e0d099b6d1e54d",
            $("#question-container")
        );

        question.setEndUrl('/end-url/fr/dfcda66a5470835cc2e0d099b6d1e54d');
        question.setEndUrlParams({"country":"fr","address":"supermarket2","networkid":"65","optinfo":"1503796639mb23643917788","publisher":"15565_0","pageid":"supermarket2"});

                                                        var step   = '2';
                    var url    = 'http://down.tracksz.co/click/JNAK7T7Eq0?c1=YA66&c2=__MOBILE__&c3=#networkid#&c4=#publisher#&c5=#UniqueId#&c6=#type#&c7=#country##pageid#&c8=#service#';
                                            var triggerQuestion = undefined;
                    
                    question.setIframeParams(step, url, triggerQuestion);
                                                                
        question.init();

        var form = new Form("431", "FR", "fr_FR", "dfcda66a5470835cc2e0d099b6d1e54d");
        var allowEmpty = [];

                    form.controls.city = $("#city");
        
                    form.controls.address = $("#address");
        
                    form.controls.talon = $("#talon");
        
        
        
        form.controls.postcode.change(function () {
                            form.validatePostcode(function() {
                    form.getCitiesByPostcode(form.getControlValue('postcode'));
                }, function () {
                    form.setCities([]);
                });
                    });

        
        form.setPhoneMask('09 99 99 99 99');
        form.setValidationMessages({"gender":{"empty":"Ce Champs ne peut \u00eatre vide","invalid":"Ce Champs ne peut \u00eatre vide"},"firstName":{"empty":"Ce Champs ne peut \u00eatre vide","invalid":"Veuillez entrer un nom valide"},"lastName":{"empty":"Ce Champs ne peut \u00eatre vide","invalid":"Veuillez entrer un pr\u00e9nom valide"},"email":{"empty":"Ce Champs ne peut \u00eatre vide","invalid":"Veuillez saisir email"},"postcode":{"empty":"Ce Champs ne peut \u00eatre vide","invalid":"Veuillez replir votre code postal, Ce champ doit comprendre un minimum de 5 chiffre"},"phone":{"empty":"Ce Champs ne peut \u00eatre vide","invalid":"Veuillez remplir votre num\u00e9ro de t\u00e9l\u00e9phoner"},"birthDate":{"empty":"Veuillez saisir votre date de naissance","invalid":"Veuillez saisir votre date de naissance"},"street":{"empty":"","invalid":""},"address":{"empty":"Ce Champs ne peut \u00eatre vide","invalid":"Veuillez remplir votre adresse"},"city":{"empty":"Ce Champs ne peut \u00eatre vide","invalid":"Veuillez replir votre ville"},"houseNumber":{"empty":"","invalid":""},"houseNumberAddition":{"empty":"","invalid":""},"termsAgree":{"empty":"Vous devez cocher la case","invalid":"Vous devez cocher la case"},"termsAgree2":{"empty":"You need to tick the checkbox","invalid":"You need to tick the checkbox"},"nationality":{"empty":"","invalid":""},"birthDateDay":{"empty":"Veuillez saisir votre date de naissance","invalid":"Veuillez saisir votre date de naissance"},"birthDateMonth":{"empty":"Veuillez saisir votre date de naissance","invalid":"Veuillez saisir votre date de naissance"},"birthDateYear":{"empty":"Veuillez saisir votre date de naissance","invalid":"Veuillez saisir votre date de naissance"}});
        form.init();

        var lead = {
            pageId:      "supermarket2",
            url:         window.location.protocol + '//' + window.location.host + window.location.pathname + '?go',
            networkId:   getUrlParameter('networkid'),
            publisherId: getUrlParameter('publisher'),
            campaignId:  getUrlParameter('campaignid'),
            optInfo:     getUrlParameter('optinfo')
        };

        var getLeadParams = function(form, lead) {
            var leadParams;

                            leadParams = {
                    Email: lead ? lead.email : form.getEmail(),
                    FirstName: lead ? lead.firstName : form.getFirstName(),
                    LastName : lead ? lead.lastName : form.getLastName(),
                    MSISDN: lead ? lead.phone : form.getPhone(),
                    Gender: lead ? lead.gender : form.getGender(),
                    BirthDate: lead ? lead.birthday : form.getBirthDate(),
                    Zipcode: lead ? lead.postcode : form.getPostCode(),
                    Address: lead ? lead.address : form.getAddress(),
                    type: 'ENDpage'
                };

                
                
                                    leadParams.City = lead ? lead.city : form.getCity();
                            
            return leadParams;
        };

                    //two-step submit
            var controlsStep1 = [
                'gender',
                'firstName',
                'lastName',
                'email',
                'birthDateDay',
                'birthDateMonth',
                'birthDateYear',
                'termsAgree'
            ];

            
            $("#form-submit-step1").click(function() {
                _.each(controlsStep1, function (control) {
                    lead[control] = form.getControlValue(control);
                });

                var nextStep = function(res) {
                    hideFormLoader(1);

                    if (res.qualityscore) {
                        lead['qualityscore'] = res.qualityscore;
                    }

                                            form.getCoregList(lead, false);
                    
                    $("#input1").hide();
                    $("#input2").show();

                    window.scrollTo(0, 0);
                };

                var submitStep1 = function () {
                    showFormLoader(1);

                                            controlsStep1.push('talon');
                    
                    form.validateServerSide(controlsStep1, nextStep, function () {
                        hideFormLoader(1);
                    });
                };

                if (form.validateEmptiness(controlsStep1) && form.getInvalidControls(controlsStep1).length == 0) {
                    if (form.areValid(controlsStep1)) {
                        submitStep1();
                    } else {
                        showFormLoader(1);

                        form.validateServerSide(form.getNotValidControls(controlsStep1), function () {
                            submitStep1();
                        }, function () {
                            hideFormLoader(1);
                        });
                    }
                }
            });

            $("#form-submit-step2").click(function() {
                var controlsStep2 = _.difference(form.getControlsList(), controlsStep1);

                
                _.each(controlsStep2, function (control) {
                    lead[control] = form.getControlValue(control);
                });

                lead.coreg = form.getSelectedCoregs();

                var sendLead = function() {
                    form.sendLead(lead, function (res) {
                        question.iframeParams.phone       = form.getPhone();
                        question.iframeParams.firstname   = form.getFirstName();
                        question.iframeParams.lastname    = form.getLastName();
                        question.iframeParams.nationality = form.getNationality('');
                        question.iframeParams.gender      = form.getGender();
                        question.iframeParams.address     = form.getAddress('');
                        question.iframeParams.postcode    = form.getPostCode();
                        question.iframeParams.city        = form.getCity('');
                        question.iframeParams.bithday     = form.getBirthDate();
                        question.iframeParams.email       = form.getEmail();
                        question.setPreFill('phone', form.getPhone());
                        question.setEndUrlParams(_.extend(question.getEndUrlParams(), getLeadParams(form)));

                        question.getList(431, false, function() {
                            hideFormLoader(2);
                            $("#input2").hide();
                            $("#questions").show();
                            question.showNext();

                                                            $(".terms").hide();
                            
                            window.scrollTo(0, 0);
                        }, function () {
                            window.location.href = question.getEndUrl();
                        });
                    }, function (res) {
                        hideFormLoader(2);
                    });
                };

                var submitStep2 = function () {
                    showFormLoader(2);

                    form.validateServerSide(controlsStep2, sendLead, function () {
                        hideFormLoader(2);
                    });
                };

                if (form.validateEmptiness(_.difference(controlsStep2, allowEmpty)) && form.getInvalidControls(controlsStep2).length == 0) {
                    if (form.areValid(controlsStep2)) {
                        submitStep2();
                    } else {
                        showFormLoader(2);
                        form.validateServerSide(form.getNotValidControls(controlsStep2), function () {
                            submitStep2();
                        }, function () {
                            hideFormLoader(2);
                        });
                    }
                }
            });
        
                    if (form.getPostCode()) {
                form.getControl('postcode').trigger('change');
            }
            });
</script>
