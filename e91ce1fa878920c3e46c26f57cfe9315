<script type="text/template" id="t-question">
    <div class="question-position question-block" id="question-<%- question.Id %>" data-type="<%- question.Structure %>" data-id="<%- question.Id %>">
        <% if (question.LogoUrl && question.LogoUrl != 'brokenimage.png') { %>
            <div class="question-logo">
                <img src="/images/sponsors/fr/<%= question.LogoUrl %>" alt="">
            </div>
        <% } %>
        <div class="question-text">
            <div class="question-title"><%= question.Header %></div>
            <% content = question.Description.replace(/<\/?[^>]+>/gi, '').replace(/&nbsp;/g, ' ').trim() %>
            <% if (content) { %>
                <div class="question-subtitle"><%= question.Description %></div>
            <% } %>
            <% if (question.Structure == "YesNoButtons" ) { %>
                <div class="q-btns">
                    <% _.each(question.Answers, function(answer) { %>
                        <button class="q-btn answer-submit <%= answer.IsPositive == '1' ? '' : 'q-btn-grey' %>" data-id="<%- answer.Id %>"><%- answer.Description %></button>
                    <% }) %>
                </div>
            <% } else if (question.Structure == "TriggerQuestion") { %>
