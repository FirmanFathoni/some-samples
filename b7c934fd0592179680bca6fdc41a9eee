(function($) {
	// fancybox all image links
	$('a').each(function() {
		var self = this;
		var file =  $(self).attr('href');
		if (file) {
			var extension = file.substr( (file.lastIndexOf('.') +1) );
			switch(extension) {
				case 'jpg':
				case 'png':
				case 'gif':
					$(self).fancybox({
						'overlayColor' : '#fff'
					});
			}
		}
	});
