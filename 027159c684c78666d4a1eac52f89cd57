// <![CDATA[
function httpsLogin(obj){
inSSID();
$('loginForm').action = 'member.login';
//$('loginForm').action = 'https://sw-login.e-wut.co.kr/t_login';
$('loginForm').action = 'https://sw-login.e-wut.co.kr/t_login';
}
function httpjoin(){
if( $('https_login').checked == true ){
//alert('SSL이 적용된 회원가입을 실시합니다.\n인증서 오류 페이지가 나올경우\n"이 웹사이트를 계속 탐색합니다" 를 선택해 주시기 바랍니다.');
//window.location.href = "https://sw-login.e-wut.co.kr/member.join_step?themeset=maesan-kg&baseurl=www.maesan.kg.kr";
window.location.href = "https://sw-login.e-wut.co.kr/member.join_step?themeset=maesan-kg&baseurl=www.maesan.kg.kr";
}else{
window.location.href = "http://sw-login.e-wut.co.kr/member.join_step?themeset=maesan-kg&baseurl=www.maesan.kg.kr";
}
}
function inSSID(){
if(!$('ssid').value) $('ssid').value = ajaxLoad("/ssid.php");
}
// ]]>
</script>
