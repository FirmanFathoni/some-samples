	jQuery(document).ready(function($){
		var timer = 3000;
		var child = 1;
		var gridLength = $('.home-grid li').length;

		var switchHover = function(){
			if ($('.home-grid img:first-child').visible())
			{
				var isHovered =  $('.home-grid:hover').length;
				if (isHovered)
				{

				}
				else
				{
					child++;
					if (child > gridLength) child = 1;
	  				$('.home-grid li').removeClass('toggled-on');
	  				$('.home-grid li:nth-child(' + child + ')').addClass('toggled-on');
	  				$('.home-grid .content').removeClass('toggled-on');
	  				$('.home-grid .content:nth-child(' + child + ')').addClass('toggled-on');
	  				clearTimeout(timeout);
	  				timeout = setTimeout(switchHover, timer);
				}
			}
			else
			{
  				clearTimeout(timeout);
  				timeout = setTimeout(switchHover, timer);
			}
		};

		// hover first element by default
		$('.home-grid li:first-child').addClass('toggled-on');
		$('.home-grid .text div:first-child').addClass('toggled-on');
		var timeout = setTimeout(switchHover, timer);

		$('.home-grid img').hover(
			function()
			{
				clearTimeout(timeout);
				$('.home-grid li').removeClass('toggled-on');
				$('.home-grid .content').removeClass('toggled-on');
				$(this).parent().addClass('toggled-on');
				var index = $(this).parent().index() + 1;
				$('.home-grid div.content:nth-child(' + index + ')').addClass('toggled-on');
			},
			function()
			{
				child = $(this).parent().index() + 1;
				clearTimeout(timeout);
				timeout = setTimeout(switchHover, timer);
			}
		);
	});
