    <script>
        $(function() {
            $('.accordion').on('show', function(e) {
                $(e.target).prev('.accordion-heading').find('.accordion-toggle').addClass('accHead_active').removeClass('accHead_unactive');
            });
            $('.accordion').on('hide', function(e) {
                $(this).find('.accordion-toggle').not($(e.target)).removeClass('accHead_active').addClass('accHead_unactive');
            });
        });
    </script>
