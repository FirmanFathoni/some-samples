<script type='text/javascript'>
(function(){
var discounts = {};
discounts.functions = {
    flat : function(amount){
        return function(bid){
            return bid - amount;
        }
    },
    percent : _WF.utils.hbDiscountTo
};
discounts.bidderSettings = { "amazon" : {
                            "bidCpmAdjustment" : discounts.functions.percent(0.99)
                        },"aol" : {
                            "bidCpmAdjustment" : discounts.functions.percent(0.84)
                        },"appnexusAst" : {
                            "bidCpmAdjustment" : discounts.functions.percent(1.00)
                        },"brealtime" : {
                            "bidCpmAdjustment" : discounts.functions.percent(0.81)
                        },"defymedia" : {
                            "bidCpmAdjustment" : discounts.functions.percent(0.83)
                        },"criteo" : {
                            "bidCpmAdjustment" : discounts.functions.percent(0.95)
                        },"pulsepoint" : {
                            "bidCpmAdjustment" : discounts.functions.percent(0.99)
                        },"openx" : {
                            "bidCpmAdjustment" : discounts.functions.percent(1.00)
                        },"indexExchange" : {
                            "bidCpmAdjustment" : discounts.functions.percent(1.00)
                        } };
;
_WF.cache.setItem(_WF.KEYS.DISCOUNTS, discounts);
})();
