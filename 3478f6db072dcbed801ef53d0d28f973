                districtMarkers = [],
                markerClusterer,
                userPosition,
                userMarker;
                          
                ymaps.ready(function () {
                
                        myMap = new ymaps.Map("salons-map", {
                            center: [55.753676, 37.619899],
                            zoom: 10
                        });
                        
                        
                        var clusterNumbers = [10,40];
                
                        myMap.controls.add(new ymaps.control.ZoomControl());
                        
                        markerClusterer = new ymaps.Clusterer({gridSize: 110, clusterNumbers: clusterNumbers});
                        
                        loadDistricts(); 
                }); 
             
                function loadDistricts() 
                {
                   districtMarkers = [];
                   $.each(districts, function(index, district) {
                   
                            districtMarkers[index] = new ymaps.Placemark([district.lat, district.lng], {
                                hintContent: htmlEncode(district.title) + ' район'
                            },
                            {
                                iconImageHref: '/img/marker.png',
                                iconImageSize: [32, 32],
                                iconImageOffset: [-16, -16]
                            });
                            
                            districtMarkers[index].events.add('click', function () {
                                searchClinics(district.id);
                            });
                            
                            myMap.geoObjects.add(districtMarkers[index]);
                    });
                }
                
                function searchClinics(districtId) 
                {
                    
                    var searchData = {
                         city_id: 1,
                         razdel_id: 398, 
                         district_id: districtId
                    };
                
                    $.ajax({
                        url: '/salon/search',
                        dataType:'json',
                        timeout: 2000,
                        data: searchData,
                        beforeSend: function() {
                            clearMarkers();
                            $('div#loadingSearch').show();	
                        },
                        success: function(data) {
                            $('div#loadingSearch').hide();	
                            if(data.success)
                            {
                                salons = data.salons,	
                                myMap.setCenter([data.geo.lat, data.geo.lng], data.geo.zoom);
                            }
                            loadClinicsMarkers();
                        },
                        error: function() {
                            $('div#loadingSearch').hide();
                        }
                    });
                        
                }
                
                function loadClinicsMarkers() 
                {
                    myMarkers = [],
                    myBalloons = [];
                    
                    $.each(salons, function(index, salon) {
                        
                        if(salon.lat && salon.lng)
                        {
                            myMarkers[index] = new ymaps.Placemark([salon.lat, salon.lng], {
                                balloonContentHeader: htmlEncode(salon.title),
                                balloonContentBody: '<div class="linkB">' + (salon.image!='' ? '<img src="' + htmlEncode(salon.image) + '_tumb.jpg" class="logo-salon">' : '') + '<a href="' + htmlEncode(salon.cityalias) + '/salon/' + htmlEncode(salon.alias) + '.html">' + htmlEncode(salon.title) + '</a>' + (salon.phone!='' ? '<div><i class="icon-volume-up"></i> ' + htmlEncode(salon.phone) + '</div>' : '') + '' + (salon.time_work!='' ? '<div><i class="icon-time"></i> ' + htmlEncode(salon.time_work) + '</div>' : '') + '</div>',
                                hintContent: htmlEncode(salon.title)
                            },
                            {
                                iconImageHref: '/img/marker.png',
                                iconImageSize: [32, 32],
                                iconImageOffset: [-16, -16]
                            });
                        }
                        
                    });
                    
                    markerClusterer.add(myMarkers);
                    myMap.geoObjects.add(markerClusterer);
                }
                
                function clearMarkers() 
                {
                    // Удаляем все  метки из кластера
                    markerClusterer.removeAll();
                    // Удаляем кластер с карты
                    myMap.geoObjects.remove(markerClusterer);
                    
                    $.each(districts, function(index, district) {
                        
                        myMap.geoObjects.remove(districtMarkers[index]);
                        
                    });
                    
                    if(userMarker)
                        myMap.geoObjects.remove(userMarker);
                }
               
               function htmlEncode(value)
               {
                    return $('<div/>').text(value).html();
               } 
                 
        });
        </script> 
        
    <script>
         $(function() {
               
                $('.ajax_search .ajax_input').keyup(function(event) {
                    var searchResult = $('#search-result');
                    if($(this).val().length>2)
                    {
                        $.get('/site/search', {text: $(this).val()}, function(data) {
                            searchResult.show().html(data);
                        });
                    }
                    else
                        searchResult.html('').hide();
                });
                
                $( ".cat img" ).load(function() {
                    var image = $(this);
                    if(image.height()<98 && image.height()!=0)
                    {
                        image.css('marginTop', ((98-image.height())/2));
                    }
                });
        });
    </script> 
    
     
