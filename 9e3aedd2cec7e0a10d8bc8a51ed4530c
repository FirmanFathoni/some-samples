    <div id="breakingNews">
        <span class="close-btn" style="float:right">X</span>
        <div class="breaking-news-header">
            <span class="breaking-news-type"></span>
        </div>
        <div class="breaking-news-body">
            <div class="breaking-news-content"></div>
            </div>
