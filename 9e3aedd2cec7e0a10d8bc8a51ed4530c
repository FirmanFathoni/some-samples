<meta name="win-tracking-landing-type" content="News" />
<script type="text/javascript">
(function(){
	
	var listenEvent = function(eventName){
		Reuters.win.event.listen(eventName, function( event, params ) {  
			fireLandingEvent(eventName, params);
		});
	};
	
	var fireLandingEvent = function(actionType, params){
		var props = {  
			"Action" : actionType,
			"Landing Type" : "News",
			"Section Name" : $('meta[name=win-tracking-section-name]').attr('content')
		};
		if(params){
			$.extend(props, params);
		}
		Reuters.win.event.fire("Landing Event", props);
	};

	var fireLandingIterEvent = function(interactionType){
		Reuters.win.event.fire("Landing Event", {  
			"Action" : "Content Interaction",
			"Landing Type" : "News",
			"Section Name" : $('meta[name=win-tracking-section-name]').attr('content'),
			"Interaction Type" : interactionType
		});
	};
	
	listenEvent("Page Load");
	listenEvent("Page Refresh");
	listenEvent("Scroll 25% Down");
	listenEvent("Scroll 50% Down");
	listenEvent("Scroll 75% Down");
	listenEvent("Scroll 100% Down");
	listenEvent("Scroll 25% Up");
	listenEvent("Scroll 50% Up");
	listenEvent("Scroll 75% Up");
	listenEvent("Scroll 100% Up");
	
	Reuters.win.event.listen("News Wire Headline Click", function( event, params ) { 
		fireLandingIterEvent("Wire");
	});	
	Reuters.win.event.listen("Load More Click", function( event, params ) {  
		fireLandingEvent("The Wire Expand");
	});
	Reuters.win.event.listen("Article Headline Click", function( event, params ) {  
		fireLandingIterEvent(params["interactionType"]);
	});
	Reuters.win.event.listen("Article Figure Click", function( event, params ) {  
		fireLandingIterEvent(params["interactionType"]);
	});
	Reuters.win.event.listen("Video Headline Click", function( event, params ) {  
		fireLandingIterEvent(params["interactionType"]);
	});
	Reuters.win.event.listen("Video Figure Click", function( event, params ) {  
		fireLandingIterEvent(params["interactionType"]);
	});
	Reuters.win.event.listen("Photo Headline Click", function( event, params ) {  
		fireLandingIterEvent(params["interactionType"]);
	});
	Reuters.win.event.listen("Photo Figure Click", function( event, params ) {  
		fireLandingIterEvent(params["interactionType"]);
	});
	Reuters.win.event.listen("More Link Click", function( event, params ) {  
		fireLandingIterEvent(params["interactionType"]);
	});
  Reuters.win.event.listen("Trending Stories Click", function(event, params) {
    fireLandingIterEvent("Trending");
  });
	
})();

</script>
