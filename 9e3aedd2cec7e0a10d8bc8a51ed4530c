<script type="text/javascript" src="https://s3.reutersmedia.net/resources_v3/js/win-main.js"></script>

<!-- analytics start -->

<script id="analyticsScript" type="text/javascript">

$(function(){
    
	var sectionName = $('meta[name=win-tracking-section-name]').attr('content');
	var pageType = $('meta[name=win-tracking-page-type]').attr('content');
	var landingType = ($('meta[name=win-tracking-page-type]').attr('content') == "Article") ? "Article" : $('meta[name=win-tracking-landing-type]').attr('content');
	var videoType = $('meta[name=win-tracking-video-page-type]').attr('content');
	
	var pageLoadSpeed = "";
	var performance = window.performance || window.webkitPerformance || window.msPerformance || window.mozPerformance;
	if(performance && performance.timing){
		pageLoadSpeed = performance.timing.domContentLoadedEventStart - performance.timing.requestStart;
	}else{
		pageLoadSpeed = "unsupported";
	}
	
	
	
	//Page View
	/*** old
	if(Reuters.utils.getFromLocalStorage("reuters.win.last.page") && Reuters.utils.getFromLocalStorage("reuters.win.last.page") == document.location.href) {
		Reuters.win.event.fire("Page View", {  
			"Action Type" : "Page Refresh",
			"Section Name" : sectionName,
			"Page Load Speed" : pageLoadSpeed,
			"Landing Type" : landingType
		});
		Reuters.win.event.fire("Page Refresh", {"Page Load Speed" : pageLoadSpeed});
	} else {
		Reuters.win.event.fire("Page View", {  
			"Action Type" : "Page Load",
			"Section Name" : sectionName,
			"Page Load Speed" : pageLoadSpeed,
			"Landing Type" : landingType
		});
		Reuters.win.event.fire("Page Load", {"Page Load Speed" : pageLoadSpeed});
	}
	***/

	if(Reuters.utils.getFromLocalStorage("reuters.win.last.page") && Reuters.utils.getFromLocalStorage("reuters.win.last.page") == document.location.href) {
		
		listenEvent("Page Refresh");
		Reuters.win.event.fire("Page Refresh", {"Page Load Speed" : pageLoadSpeed});

	} else {
			
			listenEvent("Page Load");
			Reuters.win.event.fire("Page Load", {"Page Load Speed" : pageLoadSpeed});
	}
	function listenEvent (eventName){
		Reuters.win.event.listen(eventName, function( event, params ) {  
			fireLandingEvent(eventName, params);
		});
	};
	function fireLandingEvent (actionType, params){
		var props = { "Action" : actionType,
			"Section Name" : sectionName,
			"Page Load Speed" : pageLoadSpeed,
			"Landing Type" : landingType
		};
		
		if(landingType == "Video"){
			if (videoType == "landing") { 
				var videoProps = {"Video Page Type" : "Landing"};
			}
			else {
				var videoProps = {"Video Page Type" : "Individual"};
			}
		}
		else if(landingType == "Pictures"){
			if (sectionName == "Pictures" ) { 
				var videoProps = {"Pictures Page Type" : "Landing"};
			}
			else {
				var videoProps = {"Pictures Page Type" : "Individual"};
			}
		}
		else
		{
			var videoProps = {}
		}
			
		$.extend(props, videoProps);
		if(params){
			$.extend(props, params);
		}
		Reuters.win.event.fire("Page View", props);
	}

	//Session Start&Session Ended
	var currentTime = parseInt(($.now() / 1000).toFixed(0));	//seconds
	var expire = 30*60; //seconds. expire in 30 minutes
	var expire24Hours = 24*60*60; // seconds. expire 24 hours
	var isNewSession = true;
	var sessionType = "";
	if(Reuters.utils.getFromLocalStorage("reuters.win.session.startTime") && Reuters.utils.getFromLocalStorage("reuters.win.last.pageTime")){
		var lastPageTime = Reuters.utils.getFromLocalStorage('reuters.win.last.pageTime');
		lastPageTime = parseInt(lastPageTime);
		if(currentTime - lastPageTime > expire){
			var sessionStartTime = Reuters.utils.getFromLocalStorage('reuters.win.session.startTime');
			sessionStartTime = parseInt(sessionStartTime);
			var sessionEndTime = lastPageTime + expire;
			Reuters.win.event.fire("Session Ended", {  
				"Session Time Spent" : (sessionEndTime - sessionStartTime),
				"Single Page Session" : Reuters.utils.getFromLocalStorage("reuters.win.session.singlePage"),
				"Session Start Time" : $.format.date(new Date(sessionStartTime * 1000), "yyyy-MM-dd HH:mm:ss"),
				"Session End Time" : $.format.date(new Date(sessionEndTime * 1000), "yyyy-MM-dd HH:mm:ss"),
				"Exit Page" : Reuters.utils.getFromLocalStorage("reuters.win.last.sectionName")
			});
			if (currentTime - lastPageTime < expire24Hours) {
				sessionType = "returning";
			} else {
			    sessionType = "new";
			}
		}else{
			isNewSession = false;
			Reuters.utils.setToLocalStorage("reuters.win.session.singlePage", false);
		}
	}else{
		sessionType = "new";
	}
	
	if(isNewSession){
		Reuters.utils.setToLocalStorage("reuters.win.session.startTime", currentTime);
		Reuters.utils.setToLocalStorage("reuters.win.session.singlePage", true);
		Reuters.win.event.fire("Session Start", {  
			"Session Type" : sessionType,
			"Session Time Spent" : 0,
			"Session Start Time" : $.format.date(new Date(currentTime * 1000), "yyyy-MM-dd HH:mm:ss"),
			"Entry Page" : sectionName
		});
	}
	
	//Scroll Event
	var fired25Down = false;
	var fired50Down = false;
	var fired75Down = false;
	var fired25Up = true;
	var fired50Up = true;
	var fired75Up = true;
	$(window).scroll(function() {
		var scrollTop = $(window).scrollTop();
		var scrollDistance = $(document).height() - $(window).height();
		
		if(scrollTop == 0){
			Reuters.win.event.fire("Scroll 100% Up", {});
		}
		
		if(!fired25Down && (scrollTop >= scrollDistance/4)){
      fired25Down = true;
      fired25Up = false;
			Reuters.win.event.fire("Scroll 25% Down", {});
		}
		if(!fired25Up && (scrollTop < scrollDistance/4)){
      fired25Down = false;
      fired25Up = true;
			Reuters.win.event.fire("Scroll 75% Up", {});
		}
		
		if(!fired50Down && (scrollTop >= scrollDistance/2)){
      fired50Down = true;
      fired50Up = false;
			Reuters.win.event.fire("Scroll 50% Down", {});
		}
		if(!fired50Up && (scrollTop < scrollDistance/2)){
      fired50Down = false;
      fired50Up = true;
			Reuters.win.event.fire("Scroll 50% Up", {});
		}
		
		if(!fired75Down && (scrollTop >= scrollDistance * 3/4)){
      fired75Down = true;
      fired75Up = false;
			Reuters.win.event.fire("Scroll 75% Down", {});
		}
		if(!fired75Up && (scrollTop < scrollDistance * 3/4)){
      fired75Down = false;
      fired75Up = true;
			Reuters.win.event.fire("Scroll 25% Up", {});
		}
		
		if(scrollTop == scrollDistance){
			Reuters.win.event.fire("Scroll 100% Down", {});
		}
	});
	
	var defaultViewTime =  30*60; //seconds. expire in 30 minutes
	var lastPageType = Reuters.utils.getFromLocalStorage("reuters.win.last.pageType");
	if (lastPageType === "Article" && lastPageTime) {
	    var lastArticleProps = Reuters.utils.getFromLocalStorage("reuters.win.last.articleProps");
	    if (lastArticleProps) {
	        var viewTime = currentTime - lastPageTime;
	        if (viewTime > expire24Hours) {
	            viewTime = defaultViewTime;
	        }
	        var props = {"Action": "View End", "View Time": viewTime};
	        $.extend(props, lastArticleProps);
	        Reuters.win.event.fire("Article Event", props);
	    }
	}
	
	if(pageType === "Article") {
	    Reuters.utils.setToLocalStorage("reuters.win.last.articleProps", Reuters.utils.getArticleProps());
	}
	
	Reuters.utils.setToLocalStorage("reuters.win.last.page", document.location.href);
	Reuters.utils.setToLocalStorage("reuters.win.last.sectionName", sectionName);
	Reuters.utils.setToLocalStorage("reuters.win.last.pageTime", currentTime);
	Reuters.utils.setToLocalStorage("reuters.win.last.pageType", pageType);
	
	
	//Footer Sign In Event
	Reuters.win.event.fire_link($(".section-follow-buttons span.follow-twitter").parent(), 
		"Footer Sign In Event", 
		{ "Action" : "twitter" }
	);
	Reuters.win.event.fire_link($(".section-follow-buttons span.follow-facebook").parent(), 
		"Footer Sign In Event", 
		{ "Action" : "facebook" }
	);
	Reuters.win.event.fire_link($(".section-follow-buttons span.follow-linkedin").parent(), 
		"Footer Sign In Event", 
		{ "Action" : "linkedin" }
	);
	Reuters.win.event.fire_link($(".section-follow-buttons span.follow-google").parent(), 
		"Footer Sign In Event", 
		{ "Action" : "google" }
	);
	Reuters.win.event.fire_link($(".section-follow-buttons span.follow-rss").parent(), 
		"Footer Sign In Event", 
		{ "Action" : "reuters" }
	);
	
	//Footer Events
	/**
	Reuters.win.event.fire_link($(".footer .footer-signup-download a:eq(0)"), 
		"Footer Event", 
		{ "Action" : "Newsletters" }
	);
	Reuters.win.event.fire_link($(".footer .footer-signup-download a:eq(1)"), 
		"Footer Event", 
		{ "Action" : "App Download" }
	);
	**/

  Reuters.win.event.fire_link_callback($(".footer .footer-signup-download a"), function(e){
    var para = $(e.target).data('event-para');
    Reuters.win.event.fire("Footer Event", para);
  });
	
	Reuters.win.event.fire_link_callback($(".footer .footer-links a"), function(e){
		var action = $(e.target).text();
		Reuters.win.event.fire("Footer Event", { "Action" : action });
	});
	
});
</script>
<!-- analytics end -->

