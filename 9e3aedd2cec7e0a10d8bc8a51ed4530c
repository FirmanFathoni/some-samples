  <script type="text/javascript">
    $(document).ready(function () {
        Reuters.search.enableSearchBox('searchfield');
        //Search Event
        Reuters.win.event.fire_form($("#searchForm"), function (event) {
            Reuters.win.event.fire("Search Event", {
                "Action": "user-search",
                "Search Term": $("#searchfield").val()
            });
        });
        Reuters.win.event.fire_delegate_link_callback($('#suggestion'), 'li a', function (event) {
            var li = $(event.target).closest('li');
            var searchTerm = li.attr('suggsearch');
            Reuters.win.event.fire("Search Event", {
                "Action": "auto-suggest",
                "Search Term": searchTerm
            });
        });

        //Menu Event - Logo Click
        $('.reuters-logo a').mousedown(function(){
          analytics.track('ui.interaction', {
            canonical_url: document.querySelector('[name="analyticsAttributes.canonicalUrl"]').content,
            content_type: 'Landing Page',
            content_type_channel: 'Landing - News',
            content_channel: 'World - World',
            topic_channel: 'World',
            topic_sub_channel: 'World',
            edition: 'U.S.',
            platform: 'Mobile',
            ui_element: 'Reuters Logo'
          });
        });
        //Menu Event - Expand
        $('.reuters-nav-menu').on('click', function () {
            Reuters.win.event.fire("Menu Event", {
                "Action": "Expand"
            });
        });

        //Menu Event - List Click
        $('.section-nav a:not(ul.edition-nav a)').mousedown(function(e){
          var sectionName = $(e.target).text();
          analytics.track('ui.interaction', {
            canonical_url: document.querySelector('[name="analyticsAttributes.canonicalUrl"]').content,
            content_type: 'Landing Page',
            content_type_channel: 'Landing - News',
            content_channel: 'World - World',
            topic_channel: 'World',
            topic_sub_channel: 'World',
            edition: 'U.S.',
            platform: 'Mobile',
            ui_element: sectionName
          });
        });
        
        //Edition Event
        $('ul.edition-nav a').mousedown(function(){
          analytics.track('ui.interaction', {
            canonical_url: document.querySelector('[name="analyticsAttributes.canonicalUrl"]').content,
            content_type: 'Landing Page',
            content_type_channel: 'Landing - News',
            content_channel: 'World - World',
            topic_channel: 'World',
            topic_sub_channel: 'World',
            edition: 'U.S.',
            platform: 'Mobile',
            ui_element: 'edition switcher'
          });
        });
    });

    function validateSearch() {
        var searchBlob = document.getElementById('searchfield').value;
        if (searchbox.value != "") { return true; } else { return false; }
    }
  </script>
