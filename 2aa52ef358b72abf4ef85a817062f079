<script type="text/javascript">
//<![CDATA[
    jQuery(document).ready(function($){
		;(function(element){
			var $element = $(element);
			var $slider = $('.slider', $element);
			function _runSlider(){
				$('.slider', $element).responsiver({
					interval: 0,
					speed: 800,
					start: 0,
					step: 1,
					circular: true,
					preload: true,
					fx: 'slide',
					pause: 'hover',
					control:{
						prev: '#sj_vm_slider_1325763071506686426 .control-button li[class="preview"]',
						next: '#sj_vm_slider_1325763071506686426 .control-button li[class="next"]'
					},
					getColumns: function(element){
						var match = $(element).attr('class').match(/cols-(\d+)/);
						if (match[1]){
							var column = parseInt(match[1]);
						} else {
							var column = 1;
						}
						if (!column) column = 1;
						return column;
					}
				});
			}
			setInterval(function (){
				//var $parent_slider = $element.parent();
				var $_parent_slider = $('tab-content');
				//console.log($parent_slider.height());
						//	console.log($element.parent().hasClass('selected'));
				if($element.parent().hasClass('tab-content')){
					if($element.parent().height() >= 0 ){
						if($element.parent().hasClass('selected')){
							//console.log('vao day');
						}
					}
				}
			},300);


			_runSlider() ;
