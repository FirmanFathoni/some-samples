function getPopupObject(myId)
{
   if (document.getElementById(myId))
   {
     return document.getElementById(myId);
   }
   else
   {
     return window.document[myId]; 
	}     
}
function openPDF(file)
{
windows.open(file, 'resizable,scrollbars');
}

function displayRow(){
   var row1 = document.getElementById('1hpxj');
   //var row2 = document.getElementById('1hpother');

   
   if (row1.style.display == 'none') {   
        row1.style.display = '';   
    }
   //if (row2.style.display == 'none') row2.style.display = '';

}
 
function hideRow(){
   var row1 = document.getElementById('1hpxj');
   //var row2 = document.getElementById('1hpother');

   if (row1.style.display == '') row1.style.display = 'none';
   //if (row2.style.display == '') row2.style.display = 'none';

}

function variablemsg(){
	var base = document.form1.loadbasis.value;
	if(base=='Variable'){
     alert('This functionality is under construction!');
	 document.form1.loadbasis.selectedIndex = 0;
    }

}
function getEPM(){
   //window.open('http://www.eia.doe.gov/cneaf/electricity/epm/table5_6_b.html')

    window.open('http://www.electricchoice.com/electricity-prices-by-state.php')
}
function onERTypeSelection(){
 var standard = document.form1.ertype[0].checked;
 var custom = document.form1.ertype[1].checked;

 if(standard == true){
	 document.form1.cster.style.display="none"; // this is for hiding
	 document.form1.cstbutton.style.display="none"; // this is for hiding
	 document.form1.energyrate.style.display=""; // this is for showing

 }else{
     document.form1.cster.style.display=""; // this is for showing
	 document.form1.cstbutton.style.display=""; // this is for showing
	 document.form1.energyrate.style.display="none"; // this is for hiding
 }
 onDropdownChange();
}
    
    
    
    
