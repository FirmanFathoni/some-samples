function mtminpolls(){
    document.form1.zones.selectedIndex = 0;
    var ergrate = null;

    var erType = document.form1.energyrate.value; 
    if(erType != 'Other'){// if standard energy rate is selected
       ergrate=document.form1.energyrate.value;
      }else{
       ergrate=document.form1.cster.value;
	   // Cannot be more than 1$.
	  if(ergrate>1){
       alert('Energy Rate should not exceed $1.0/kWh.');
       ergrate = '0.08';
	   document.form1.cster.value = '0.08';
       }
    }
    
    xjdataminpolls(ergrate);
    
