                hpSelected = '2';
        else if(document.form1.hp[2].checked == true)
                hpSelected = '3';
        else if(document.form1.hp[3].checked == true)
                hpSelected = '4';
        else if(document.form1.hp[4].checked == true)
                hpSelected = '5';
        else
                hpSelected = '6';
                       
        
        if(document.form1.temprange[0].checked == true)
                document.form1.boxtemp.value = 25;
        else
                document.form1.boxtemp.value = -20;
                	
        
        //alert('In getStandardFeature');
	//var xmlHttpRequest = getXmlHttpRequest();
	var url = "/xline/ElecAndMechSpecification?temp=" + escape(document.form1.boxtemp.value)+ "&energyRate="+ escape(ergrate)+
                                                "&zones="+ escape(document.form1.zones.value)+"&hp="+ escape(hpSelected);		
 	
                                                
        win2 = window.open(url);// Name of wondow ia removed, so multiple instance of window is generated.
        win2.focus();                                                
                                                
                                                
