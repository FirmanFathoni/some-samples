    function printIt()
  {
  
        
	var currentTime = new Date()
	var month = currentTime.getMonth() + 1
	var day = currentTime.getDate()
	var year = currentTime.getFullYear()
	//win.document.write(month + "/" + day + "/" + year)
	
  
    var inputs = document.getElementsByName("temprange");
    var trange='';//document.form1.temprange.value;
    for (var i = 0; i < inputs.length; i++) {
         if (inputs[i].checked) { 
                trange = inputs[i].value;
         }
    }
    //alert(trange);
     var zone=document.form1.zones.value;
    var ergrate = null;
    var erType = document.form1.energyrate.value; 
    if(erType != 'Other'){// if standard energy rate is selected
      ergrate=document.form1.energyrate.value;
    }else{
      ergrate=document.form1.cster.value;
	  }
	var imagetag ='<img src=\"Perf.jpg\" width=\"15\" height=\"15\" align=\"absmiddle\" border=\"0\">';
    
    var starttag = '<html><head><title>Copeland Scroll Outdoor Condensing Unit</title>';
	var styletag = '<STYLE type=text/css>'+
                   '.settingTable{ border : 1px #ccc solid;}'+
                   '.bold {	FONT-WEIGHT: bold}'+
                   '.erdropDown { FONT-WEIGHT: normal; FONT-SIZE: 10pt; COLOR: #000000; FONT-FAMILY: Arial}'+
