function changeTitle(obj){
    
    if(obj.value == '35'){
        document.getElementById('xjtabletitle').innerHTML = 'Annual Cost and Saving Summary at 25&deg;F Evaporator Temp.';
        document.form1.hp[0].checked = true;
        displayRow();
    }else{
        document.getElementById('xjtabletitle').innerHTML = 'Annual Cost and Saving Summary at -20&deg;F Evaporator Temp.';
        document.form1.hp[1].checked = true;
        hideRow();
    }
    
   // alert("00 "+document.form1.hp[0].checked);
    //alert("11 "+document.form1.hp[1].checked);
}

//cursorType is a string.  Valid values are predefined CSS2 cursors, such as "help", "crosshair", etc

      function isNumberKey(evt)
      {
         var charCode = (evt.which) ? evt.which : event.keyCode
         if ( charCode == 46 || charCode == 48 ||charCode == 49|| charCode == 50 ||charCode == 51 || charCode == 52 ||
		      charCode == 53 || charCode == 54 ||charCode == 55 || charCode == 56 ||charCode == 57  )
            return true;

         return false;
		 
      }
  var win=null;
  
  

    var popupWindow=null;

    
    function MailReport(){
    
      document.form1.report.value="";            
      //var standard = document.form1.ertype[0].checked;
     // var custom = document.form1.ertype[1].checked;
      document.form1.report.value="YES";
    
        
        
        if(document.form1.temprange[0].checked == true)
                document.form1.boxtemp.value = 25;
        else
                document.form1.boxtemp.value = -20;
        
               
         var erType = document.form1.energyrate.value;
         
         if(erType != 'Other'){
         
                 document.form1.energyRate.value=document.form1.energyrate.value;

         }else{

                document.form1.energyRate.value=document.form1.cster.value;	 
         }
         
    //document.all['ALERTCOVER'].style.visibility="visible";
 
    var width = 600;
    var height = 310;
    var left = parseInt((screen.availWidth/2) - (width/2));
    var top = parseInt((screen.availHeight/2) - (height/2));
    
    var windowFeatures = "width=" + width + ",height=" + height + ",status,resizable=no,left=" + left + ",top=" + top + "screenX=" + left + ",screenY=" + top;
    
    //document.form1.action
    //document.form1.submit();
    //window.open(document.getElementById('mailWindow').style.visibility = "visible")
    popupWindow = window.open ("MailWindow.html","MailWindow",windowFeatures);
    //popupWindow = window.showModalDialog ("MailWindow.html","MailWindow",'resizable: no;unadorned:yes;resizable:0;dialogHeight:310px;dialogwidth:600px;scroll:no;status=no;dialogLeft:340px;dialogTop:342px');
    //popupWindow = window.open ("MailWindow.html","MailWindow",windowFeatures);
    popupWindow.focus();
    
    }
    
   
    
    function validate(){
    
            if(document.mailForm.from.value == "")
            {
                    alert("Please enter your mail ID");
                    return false;
            }
            
            if(document.mailForm.to.value == "")
            {
                    alert("Please enter receipents mail ID");
                    return false;
            }
            
            if(document.mailForm.subject.value == "")
            {
                    alert("Subject should not be blank");
                    return false;
            }

    
            document.form1.from.value=document.mailForm.from.value;
            document.form1.to.value=document.mailForm.to.value;
            document.form1.subject.value=document.mailForm.subject.value;
           // document.form1.body.value=document.mailForm.body.value;
    
            document.all['ALERTCOVER'].style.visibility="hidden";
            
            document.forms[0].submit();
            
            return true;
    }
    
    function hideMailWindow()
    {
        document.mailForm.from.value = "";
        document.mailForm.to.value = ""
        document.mailForm.subject.value = ""
        //document.mailForm.body.value = ""
        
        document.all['ALERTCOVER'].style.visibility="hidden";
    }


function createPopup() {
	//Get the data from the form fields
	var background = "";
	var title =  "";
	var text =  "";
	
	//Now create the HTML code that is required to make the popup
	var content = "<html><head><title>Hello</title></head>\
<body bgcolor='"+background+"'><h1>Hello</h1>"+text+"<br />\
<form name=mailForm  method=post>\
<table width=70% border=1 align=center>\
            <tr>\
                <td colspan=1><div  id=from  align=center><b>From</div></td>\
                <td colspan=3><input type=text name=cc size=60 align=middle ></td>\
            </tr>\
            <tr>\
                <td colspan=2 align=right ><input type=button value=Send size=35 onclick=javascript:assignValues();> </td>\
                <td colspan=2 align=left><input type=button size=35 value=Close onclick=self.close()> </td>\
            </tr>\
<a href='javascript:window.close()'>Close the popup</a></form></body></html>";

	//Create the popup
        var width = 600;
        var height = 330;
    var left = parseInt((screen.availWidth/2) - (width/2));
    var top = parseInt((screen.availHeight/2) - (height/2));
    var windowFeatures = "width=" + width + ",height=" + height + ",status,resizable,left=" + left + ",top=" + top + "screenX=" + left + ",screenY=" + top;
    
	var popup = window.open("","window",windowFeatures);
	popup.document.write(content); //Write content into it.
	//pops.document.close();
        
        
}

function alterCursor(elementId, cursorType)
{
	var myElement=getPopupObject(elementId);
	if (myElement)
	{
		myElement.style.cursor=cursorType;	
	}
}
function getEnergycost(standardCost, energyrate){

 var energyCost = Math.round( standardCost * energyrate );
 
 return addCommas(energyCost);
}
function getCostSavings(unitopcost, xjopcost, energyrate){

 var unitCost = Math.round( unitopcost * energyrate );

 var xjCost = Math.round( xjopcost * energyrate );

 var savings = unitCost - xjCost;

 if( savings < 0){
 savings = Math.abs(savings);
 savings ='('+savings+')';
}else{

 }
 
 return addCommas(savings);
}
function addCommas(nStr)
{
	nStr += '';
	x = nStr.split('.');
	x1 = x[0];
	x2 = x.length > 1 ? '.' + x[1] : '';
	var rgx = /(\d+)(\d{3})/;
	while (rgx.test(x1)) {
		x1 = x1.replace(rgx, '$1' + ',' + '$2');
	}
	return x1 + x2;
}

    function txtVisible(){
    
        var ergrate=document.form1.energyrate.value;


        if(ergrate == 'Other'){// if standard energy rate is selected
    //alert('Other');
            document.form1.cster.style.visibility = 'visible';
      }else{
     // alert('Not Other');
            document.form1.cster.style.visibility = 'hidden';
       }

    }
    
function onDropdownChange()
{  
    
    
    var temp = "";

        if(document.form1.temprange[0].checked == true)
                temp = 25;
        else
                temp = -20;

       var applabel= document.getElementById('apptype');

       

       var zone = document.form1.zones.value;

        if(zone=='Zone 1'){
          mtminpolls();
         }
        if(zone=='Zone 2'){
         mtdayton();
         }
        if(zone=='Zone 3'){
         mtstlouis();
         }
        if(zone=='Zone 4'){
         mtatlanta();
         }
        if(zone=='Zone 5'){
          mtmiami();
         }
       onTempChange();
       
}

    function onZoneDropdownChange(){
      var zone = document.form1.zones.value;
        if(zone=='Zone 1'){
          mtminpolls();
         }
        if(zone=='Zone 2'){
         mtdayton();
         }
        if(zone=='Zone 3'){
         mtstlouis();
         }
        if(zone=='Zone 4'){
         mtatlanta();
         }
        if(zone=='Zone 5'){
          mtmiami();
         }
    }

function onUnitDropdownChange()
{  
   var applabel= document.getElementById('apptype');
   var unit = document.form1.unitrange.value;
   
   if( unit=='mediumtemp'){
    document.getElementById('apptype').innerHTML = 'Box Temperature (°F)<br>(Walk-In Cooler):';
	document.form1.temprange.selectedIndex = 0;
	displayRow();
   }
   if( unit=='lowtemp'){
    document.getElementById('apptype').innerHTML = 'Box Temperature (°F)<br>(Walk-In Freezer):';
	document.form1.temprange.selectedIndex = 1;
	hideRow();
    }

    var zone = document.form1.zones.value;
    if(zone=='Zone 1'){
      mtminpolls();
     }
    if(zone=='Zone 2'){
     mtdayton();
     }
    if(zone=='Zone 3'){
     mtstlouis();
     }
    if(zone=='Zone 4'){
     mtatlanta();
     }
    if(zone=='Zone 5'){
      mtmiami();
     }
   onTempChange();
}
function on1hpclick(){
	 var temp = "";

        if(document.form1.temprange[0].checked == true)
                temp = 25;
        else
                temp = -20;
                
	 if(temp=='25'){
     document.getElementById('1hp').onClick  = window.open('energycalc/MediumTemp/XJAM-015Z-CFV-022.pdf');
	 }else{
     document.getElementById('1hp').onClick  = alert('Condensing Unit is not available for the selected Capacity!');  
     }
}
