  function selectCDUType(cdutype){
  
   // alert('CDUType ==> '+cdutype);
    if(cdutype == 'SS'){
    
        document.form1.isscroll.value = 'true';
        document.form1.isrecip.value = 'false';
        document.form1.issemi.value = 'false';        
        
    } else if(cdutype == 'HR'){
    
        document.form1.isscroll.value = 'false';
        document.form1.isrecip.value = 'true';
        document.form1.issemi.value = 'false';
        
    }else{
        
        document.form1.isscroll.value = 'false';
        document.form1.isrecip.value = 'false';
        document.form1.issemi.value = 'true';
    
    }
    
    //alert("isscroll==>"+document.form1.isscroll.value);
   // alert("isrecip==>"+document.form1.isrecip.value);
   // alert("issemi==>"+document.form1.issemi.value);
  
  }
  
  
  //Ajax
  
  function getXmlHttpRequest()
{
	var xmlHttpRequest;
	if(window.XMLHttpRequest){
		xmlHttpRequest = new XMLHttpRequest();
	
	}
	else if(window.ActiveXObject){
		xmlHttpRequest = new ActiveXObject("Microsoft.XMLHTTP");	
	}
		return  xmlHttpRequest;;
}
  var win1='';
  var win2='';
  var win3='';
  function getStandardFeature(){	
            
       var ergrate = null;
       var erType = document.form1.energyrate.value; 
       
        if(erType != 'Other'){// if standard energy rate is selected
            ergrate=document.form1.energyrate.value;
        }else{
            ergrate=document.form1.cster.value;
        }
          
        var hpSelected = "";
        
        if(document.form1.hp[0].checked == true)
                hpSelected = '1';
        else if(document.form1.hp[1].checked == true)
                hpSelected = '2';
        else if(document.form1.hp[2].checked == true)
                hpSelected = '3';
        else if(document.form1.hp[3].checked == true)
                hpSelected = '4';
        else if(document.form1.hp[4].checked == true)
                hpSelected = '5';
        else
                hpSelected = '6';
                       
        
        if(document.form1.temprange[0].checked == true)
                document.form1.boxtemp.value = 25;
        else
                document.form1.boxtemp.value = -20;
                	
                	
	var url = "/xline/StandardFeature?temp=" + escape(document.form1.boxtemp.value)+ "&energyRate="+ escape(ergrate)+
                                                "&zones="+ escape(document.form1.zones.value)+"&hp="+ escape(hpSelected);		
 	
          
       //document.form1.method = 'post';                                                
       //document.form1.action = url;
       //document.form1.submit();\
       
       //win1 = window.open(url, 'StandardFeature'); // This opens only one instance of w1ndow for StdFeature.
       
       win1 = window.open(url); // Name of wondow ia removed, so multiple instance of window is generated.
       win1.focus();
       //alert(window.parent.location);
       
       

}


