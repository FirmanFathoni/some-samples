function onrecipselect(){
    if(document.form1.recipcheck.checked == false){
      document.form1.recipcheck.checked = true;
	}else{
	document.form1.scrollcheck.checked = false;
	document.form1.semicheck.checked = false;
 
        document.getElementById('p1hp').title = 'View 1.5 HP Hermetic Reciprocating Unit\'s Performance';
        document.getElementById('p2hp').title = 'View 2 HP Hermetic Reciprocating Unit\'s Performance';
	document.getElementById('p3hp').title = 'View 3 HP Hermetic Reciprocating Unit\'s Performance';
	document.getElementById('p4hp').title = 'View 4 HP Hermetic Reciprocating Unit\'s Performance';
	document.getElementById('p5hp').title = 'View 5 HP Hermetic Reciprocating Unit\'s Performance';
	document.getElementById('p6hp').title = 'View 6 HP Hermetic Reciprocating Unit\'s Performance';
    }
 }
 function onscrollselect(){
    if(document.form1.scrollcheck.checked == false){
      document.form1.scrollcheck.checked = true;
	}else{
	document.form1.recipcheck.checked = false;
	document.form1.semicheck.checked = false;

	document.getElementById('p1hp').title = 'View 1.5 HP Standard Scroll Unit\'s Performance';
	document.getElementById('p2hp').title = 'View 2 HP Standard Scroll Unit\'s Performance';
	document.getElementById('p3hp').title = 'View 3 HP Standard Scroll Unit\'s Performance';
	document.getElementById('p4hp').title = 'View 4 HP Standard Scroll Unit\'s Performance';
	document.getElementById('p5hp').title = 'View 5 HP Standard Scroll Unit\'s Performance';
	document.getElementById('p6hp').title = 'View 6 HP Standard Scroll Unit\'s Performance';
    }
}
function onsemiselect(){
    if(document.form1.semicheck.checked == false){
      document.form1.semicheck.checked = true;
	}else{
	document.form1.recipcheck.checked = false;
	document.form1.scrollcheck.checked = false;

        document.getElementById('p1hp').title = 'View 1.5 HP Semi-Hermetic Unit\'s Performance';
	document.getElementById('p2hp').title = 'View 2 HP Semi-Hermetic Unit\'s Performance';
	document.getElementById('p3hp').title = 'View 3 HP Semi-Hermetic Unit\'s Performance';
	document.getElementById('p4hp').title = 'View 4 HP Semi-Hermetic Unit\'s Performance';
	document.getElementById('p5hp').title = 'View 5 HP Semi-Hermetic Unit\'s Performance';
	document.getElementById('p6hp').title = 'View 6 HP Semi-Hermetic Unit\'s Performance';
    }
}

 function mtatlanta(){
   document.form1.zones.selectedIndex = 3;
   var ergrate = null;

   var erType = document.form1.energyrate.value; 
   
   if(erType != 'Other'){// if standard energy rate is selected
      ergrate=document.form1.energyrate.value;
   }else{
      ergrate=document.form1.cster.value;
	  // Cannot be more than 1$
	  if(ergrate>1){
       alert('Energy Rate should not exceed $1.0/kWh.');
       ergrate = '0.08';
	   document.form1.cster.value = '0.08';
	  }
   }
   
    xjdataAtlanta(ergrate);
    
