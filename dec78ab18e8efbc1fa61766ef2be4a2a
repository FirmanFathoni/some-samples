function onper1hpselection(){
        
        
         var temp = "";

        if(document.form1.temprange[0].checked == true)
                temp = 25;
        else
                temp = -20;
                
	 var isscroll = document.form1.isscroll.value;        
	var isrecip = document.form1.isrecip.value;	
	var issemi = document.form1.issemi.value;
        
        //alert('isscroll==>'+isscroll);
        
	 if(temp=='25'){
		 if(isrecip == 'true'){
                        window.open('energycalc/MediumTemp/FJAM-A175-CFV-020.pdf');
		 }else if(isscroll == 'true'){                        
                        window.open('energycalc/MediumTemp/FJAM-A15Z-CFV-172.pdf');
		 }else{
                        window.open('energycalc/MediumTemp/CJAM-0200-CAV-001.pdf');
		 }
       }else{
                        alert('Condensing Unit is not available for the selected Capacity!');  
       }
}

function onper2hpselection(){
	var temp = "";

        if(document.form1.temprange[0].checked == true)
                temp = 25;
        else
                temp = -20;
                
        var isscroll = document.form1.isscroll.value;        
	var isrecip = document.form1.isrecip.value;	
	var issemi = document.form1.issemi.value;

	 if(temp=='25'){
		 if(isrecip == 'true'){
                        window.open('energycalc/MediumTemp/FJAM-A225-TFC-015.pdf');
		 }else if(isscroll == 'true'){
                        window.open('energycalc/MediumTemp/FJAM-X20Z-TFC-01X.pdf');
		 }else{
                        window.open('energycalc/MediumTemp/D8AM-0201-TAC-020.pdf');
		 }
     
	 }else{
         if(isrecip == 'true'){
                    window.open('energycalc/LowTemp/FJAL-B301-CFV-015.pdf');
		 }else if(isscroll == 'true'){
                    window.open('energycalc/LowTemp/DJAL-020Z-CFV-015.pdf');
		 }else{
                    alert('Condensing Unit is not available for the selected Capacity!');  
		 }
     }
}
