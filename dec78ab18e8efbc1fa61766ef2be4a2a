function getJobInfo(){

     var ergrate = null;
       var erType = document.form1.energyrate.value; 
       
        if(erType != 'Other'){// if standard energy rate is selected
            ergrate=document.form1.energyrate.value;
        }else{
            ergrate=document.form1.cster.value;
        }
          
        document.form1.energyRate.value = ergrate;
        
        var hpSelected = "";
        
        if(document.form1.hp[0].checked == true)
                hpSelected = '1';
        else if(document.form1.hp[1].checked == true)
                hpSelected = '2';
        else if(document.form1.hp[2].checked == true)
                hpSelected = '3';
        else if(document.form1.hp[3].checked == true)
                hpSelected = '4';
        else if(document.form1.hp[4].checked == true)
                hpSelected = '5';
        else
                hpSelected = '6';
                       
        document.form1.hpSelected.value = hpSelected;
                
        if(document.form1.temprange[0].checked == true)
                document.form1.boxtemp.value = 25;
        else
                document.form1.boxtemp.value = -20;                	                		         	
                                                
        win3 = window.open("JobInfo.html");// Name of wondow ia removed, so multiple instance of window is generated.
        win3.focus();

}
  
function toggleToc() {
        var toc = document.getElementById("toc").getElementsByTagName("ul")[0];
        var toggleLink = document.getElementById("togglelink");
        if (toc && toggleLink && toc.style.display == "none") {
            toggleLink.innerHTML = "hide";
            toc.style.display = "block";
        } else {
            toggleLink.innerHTML = "show";
            toc.style.display = "none";
        }
    }  
    
    </script>
    
